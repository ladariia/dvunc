, { useContext, useEffect, useState } from 'react'\nimport { BrowserRouter } from 'react-router-dom';\nimport AppRouter from './components/AppRouter';\nimport NavBar from './components/NavBar';\nimport { Context } from './index';\nimport { observer } from 'mobx-react-lite';\nimport { Spinner } from 'react-bootstrap';\nimport { check } from './http/userAPI'\n\nconst App = observer(() => {\n  const { user } = useContext(Context)\n  const [loading, setLoading] = useState(true)\n\n  useEffect(() => {\n    check().then(data => {\n      user.setUser(true)\n      user.setIsAuth(true)\n    }).finally(() => setLoading(false))\n  }, [])\n\n  if (loading) {\n    return <Spinner animation={\"grow\"} />\n  }\n\n  return (\n    <BrowserRouter>\n      <NavBar />\n      <AppRouter />\n    </BrowserRouter>\n  );\n})\n\nexport default App;\n","/Users/darialadilova/Desktop/AUTH/client/src/components/AppRouter.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/routes.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/pages/Courses.js",["101","102"],[],"/Users/darialadilova/Desktop/AUTH/client/src/pages/Admin.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/pages/CoursePage.js",["103","104"],[],"/Users/darialadilova/Desktop/AUTH/client/src/pages/Auth.js",["105"],[],"import React, { useState, useContext } from 'react';\nimport { Context } from '../index';\nimport { Card, Container, Form, Button } from 'react-bootstrap';\nimport { LOGIN_ROUTE, REGISTRATION_ROUTE } from '../utils/consts';\nimport { NavLink, useLocation, useNavigate } from 'react-router-dom';\nimport { registration, login } from \"../http/userAPI\";\nimport { observer } from 'mobx-react-lite';\nimport { ADMIN_ROUTE } from '../utils/consts'\n\nconst Auth = observer(() => {\n    const location = useLocation() //получить маршрут в строке запроса\n    const isLogin = location.pathname === LOGIN_ROUTE\n    const [user_login, setLogin] = useState('')\n    const [user_password, setPassword] = useState('')\n    const { user } = useContext(Context)\n    const navigate = useNavigate()\n\n    const click = async () => {\n        try {\n            let userData\n            if (isLogin) {\n                userData = await login(user_login, user_password)\n            } else {\n                userData = await registration(user_login, user_password)\n            }\n            user.setUser(user)\n            user.setIsAuth(true)\n            navigate(ADMIN_ROUTE)\n        } catch (error) {\n            alert(error.response.data.message)\n        }\n    }\n\n    return (\n        <Container className=\"d-flex justify-content-center align-items-center\" style={{ height: window.innerHeight - 54 }}>\n            <Card style={{ width: 600 }} className=\"p-5\">\n                <h2 className=\"m-auto\">{isLogin ? 'Авторизация' : \"Регистрация\"}</h2>\n                <Form className=\"d-flex flex-column\">\n                    <Form.Control\n                        className=\"mt-3\"\n                        placeholder=\"Логин\"\n                        value={user_login}\n                        onChange={e => setLogin(e.target.value)}\n                    />\n                    <Form.Control\n                        className=\"mt-3\"\n                        placeholder=\"Пароль\"\n                        value={user_password}\n                        onChange={e => setPassword(e.target.value)}\n                        type=\"password\"\n                    />\n                    <div className=\"d-flex justify-content-between mt-3 pl-3 pr-3\">\n                        {isLogin ?\n                            <div className=\"d-flex\">\n                                Нет аккаунта?&nbsp;<NavLink to={REGISTRATION_ROUTE}>Зарегистрироваться</NavLink>\n                            </div>\n                            :\n                            <div className=\"d-flex\">\n                                Есть аккаунт?&nbsp;<NavLink to={LOGIN_ROUTE}>Войти</NavLink>\n                            </div>\n                        }\n                        <Button onClick={click} variant={\"dark\"}>\n                            {isLogin ? 'Войти' : 'Регистрация'}\n                        </Button>\n                    </div>\n                </Form>\n            </Card>\n        </Container>\n    );\n});\n\nexport default Auth;","/Users/darialadilova/Desktop/AUTH/client/src/utils/consts.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/store/UserStore.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/store/CourseStore.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/components/NavBar.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/components/TypeBar.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/components/CourseList.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/components/CourseItem.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/components/modals/CreateCourse.js",["106"],[],"/Users/darialadilova/Desktop/AUTH/client/src/http/userAPI.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/http/index.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/http/courseAPI.js",[],[],{"ruleId":"107","severity":1,"message":"108","line":19,"column":6,"nodeType":"109","endLine":19,"endColumn":8,"suggestions":"110"},{"ruleId":"107","severity":1,"message":"111","line":14,"column":8,"nodeType":"109","endLine":14,"endColumn":10,"suggestions":"112"},{"ruleId":"107","severity":1,"message":"111","line":20,"column":8,"nodeType":"109","endLine":20,"endColumn":29,"suggestions":"113"},{"ruleId":"114","severity":1,"message":"115","line":2,"column":10,"nodeType":"116","messageId":"117","endLine":2,"endColumn":16},{"ruleId":"107","severity":1,"message":"118","line":11,"column":8,"nodeType":"109","endLine":11,"endColumn":10,"suggestions":"119"},{"ruleId":"114","severity":1,"message":"120","line":24,"column":17,"nodeType":"116","messageId":"117","endLine":24,"endColumn":25},{"ruleId":"107","severity":1,"message":"111","lin