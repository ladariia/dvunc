s",["101"],[],"import React, { useContext, useEffect, useState } from 'react'\nimport { BrowserRouter } from 'react-router-dom';\nimport AppRouter from './components/AppRouter';\nimport NavBar from './components/NavBar';\nimport { Context } from './index';\nimport { observer } from 'mobx-react-lite';\nimport { Spinner } from 'react-bootstrap';\nimport { check } from './http/userAPI'\n\nconst App = observer(() => {\n  const { user } = useContext(Context)\n  const [loading, setLoading] = useState(true)\n\n  useEffect(() => {\n    check().then(data => {\n      user.setUser(true)\n      user.setIsAuth(true)\n    }).finally(() => setLoading(false))\n  }, [])\n\n  if (loading) {\n    return <Spinner animation={\"grow\"} />\n  }\n\n  return (\n    <BrowserRouter>\n      <NavBar />\n      <AppRouter />\n    </BrowserRouter>\n  );\n})\n\nexport default App;\n","/Users/darialadilova/Desktop/AUTH/client/src/components/AppRouter.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/routes.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/pages/Courses.js",["102"],[],"/Users/darialadilova/Desktop/AUTH/client/src/pages/Admin.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/pages/CoursePage.js",["103"],[],"import React, { useEffect, useState } from 'react';\nimport { Button, Col, Container, Row, Card } from 'react-bootstrap';\nimport { useParams } from 'react-router-dom';\nimport { fetchCourse } from '../http/courseAPI';\n\nconst CoursePage = () => {\n    const [course, setCourse] = useState({ course_module: [], course_shedule: [] })\n    const { course_id } = useParams()\n    useEffect(() => {\n        fetchCourse(course_id).then(data => setCourse(data))\n    }, [])\n    return (\n        <Container>\n            <Row style={{ marginTop: 200 }}>\n                <Col md={2}>type</Col>\n            </Row>\n            <Row>\n                <Col md={12}>\n                    <h1>{course.course_name}</h1>\n                </Col>\n            </Row>\n            <Row>\n                <Col md={2}>\n                    <Button variant=\"dark\">Подать заявку</Button>\n                </Col>\n                <Col md={2}>close date</Col>\n            </Row>\n            <Row style={{ marginTop: 50 }}>\n                <Col md={3}>format</Col>\n                <Col md={3}>{course.course_price} рублей</Col>\n                <Col md={3}>{course.course_duration} часов</Col>\n                <Col md={3}>\n                    {course.course_shedule.map(shedule =>\n                        <Row key={shedule.shedule_id} >\n                            <div>{shedule.shedule_dateofstart} - {shedule.shedule_dateoffinish}</div>\n                        </Row>\n                    )}\n                </Col>\n            </Row>\n            <Row style={{ marginTop: 200, marginBottom: 100 }}>\n                <Card className=\"p-4\" style={{ boxShadow: '0px 8px 32px rgba(0, 0, 0, 0.08)', border: 'none' }}>\n                    <h2 style={{ marginBottom: 50 }}>План программы</h2>\n                    {course.course_module.map(module =>\n                        <Row key={module.module_id} >\n                            <h3 style={{ paddingLeft: 200 }}>{module.module_name}</h3>\n                        </Row>\n                    )}\n                </Card>\n            </Row>\n        </Container>\n    );\n};\n\nexport default CoursePage;","/Users/darialadilova/Desktop/AUTH/client/src/pages/Auth.js",["104"],[],"import React, { useState, useContext } from 'react';\nimport { Context } from '../index';\nimport { Card, Container, Form, Button } from 'react-bootstrap';\nimport { LOGIN_ROUTE, REGISTRATION_ROUTE } from '../utils/consts';\nimport { NavLink, useLocation, useNavigate } from 'react-router-dom';\nimport { registration, login } from \"../http/userAPI\";\nimport { observer } from 'mobx-react-lite';\nimport { ADMIN_ROUTE } from '../utils/consts'\n\nconst Auth = observer(() => {\n    const location = useLocation() //получить маршрут в строке запроса\n    const isLogin = location.pathname === LOGIN_ROUTE\n    const [user_login, setLogin] = useState('')\n    const [user_password, setPassword] = useState('')\n    const { user } = useContext(Context)\n    const navigate = useNavigate()\n\n    const click = async () => {\n        try {\n            let userData\n            if (isLogin) {\n                userData = await login(user_login, user_password)\n            } else {\n                userData = await registration(user_login, user_password)\n            }\n            user.setUser(user)\n            user.setIsAuth(true)\n            navigate(ADMIN_ROUTE)\n        } catch (error) {\n            alert(error.response.data.message)\n        }\n    }\n\n    return (\n        <Container className=\"d-flex justify-content-center align-items-center\" style={{ height: window.innerHeight - 54 }}>\n            <Card style={{ width: 600 }} className=\"p-5\">\n                <h2 className=\"m-auto\">{isLogin ? 'Авторизация' : \"Регистрация\"}</h2>\n                <Form className=\"d-flex flex-column\">\n                    <Form.Control\n                        className=\"mt-3\"\n                        placeholder=\"Логин\"\n                        value={user_login}\n                        onChange={e => setLogin(e.target.value)}\n                    />\n                    <Form.Control\n                        className=\"mt-3\"\n                        placeholder=\"Пароль\"\n                        value={user_password}\n                        onChange={e => setPassword(e.target.value)}\n                        type=\"password\"\n                    />\n                    <div className=\"d-flex justify-content-between mt-3 pl-3 pr-3\">\n                        {isLogin ?\n                            <div className=\"d-flex\">\n                                Нет аккаунта?&nbsp;<NavLink to={REGISTRATION_ROUTE}>Зарегистрироваться</NavLink>\n                            </div>\n                            :\n                            <div className=\"d-flex\">\n                                Есть аккаунт?&nbsp;<NavLink to={LOGIN_ROUTE}>Войти</NavLink>\n                            </div>\n                        }\n                        <Button onClick={click} variant={\"dark\"}>\n                            {isLogin ? 'Войти' : 'Регистрация'}\n                        </Button>\n                    </div>\n                </Form>\n            </Card>\n        </Container>\n    );\n});\n\nexport default Auth;","/Users/darialadilova/Desktop/AUTH/client/src/utils/consts.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/store/UserStore.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/store/CourseStore.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/components/NavBar.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/components/TypeBar.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/components/CourseList.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/components/CourseItem.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/components/modals/CreateCourse.js",["105"],[],"/Users/darialadilova/Desktop/AUTH/client/src/http/userAPI.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/http/index.js",[],[],"/Users/darialadilova/Desktop/AUTH/client/src/http/courseAPI.js",[],[],{"ruleId":"106","severity":1,"message":"107","line":19,"column":6,"nodeType":"108","endLine":19,"endColumn":8,"suggestions":"109"},{"ruleId":"106","severity":1,"message":"110","line":14,"column":8,"nodeType":"108","endLine":14,"endColumn":10,"suggestions":"111"},{"ruleId":"106","severity":1,"message":"112","line":11,"column":8,"nodeType":"108","endLine":11,"endColumn":10,"suggestions":"113"},{"ruleId":"114","severity":1,"message":"115","line":24,"column":17,"nodeType":"116","messageId":"117","endLine":24,"endColumn":25},{"ruleId":"106","severity":1,"message":"110","line":19,"column":8,"nodeType":"108","endLine":19,"endColumn":10,"suggestions":"118"},"react-hook